{
  "name": "vue-cookieconsent",
  "version": "0.0.7",
  "description": "A Vue plugin, that wraps the awesome `cookieconsent` library.",
  "main": "./lib/src/index.js",
  "files": [
    "lib/**/*",
    "src/**/*",
    "vendor/**/*"
  ],
  "scripts": {
    "build": "tsc && cp ./vendor/cookieconsent.css ./lib/vendor/cookieconsent.css",
    "clean": "rm -rf ./lib/",
    "lint": "eslint ./src/ --fix",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eyecatchup/vue-cookieconsent.git"
  },
  "license": "MIT",
  "author": "Stephan Schmitz <eyecatchup@gmail.com>",
  "engines": {
    "node": ">=12.0"
  },
  "homepage": "https://github.com/eyecatchup/vue-cookieconsent",
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.5.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "types": "./lib/src/index.d.ts",
  "bugs": {
    "url": "https://github.com/eyecatchup/vue-cookieconsent/issues"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "gdpr",
    "dsgvo",
    "cookies",
    "cookie consent",
    "cookiebanner",
    "cookie banner"
  ]
}
